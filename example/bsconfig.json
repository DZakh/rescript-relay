{
  "name": "reason-react-examples",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bsc-flags": ["-bs-super-errors", "-bs-no-version-header"],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "ppx-flags": ["rescript-relay/ppx"],
  "namespace": true,
  "bs-dependencies": [
    "@rescript/react",
    "rescript-relay",
    "@glennsl/rescript-fetch"
  ]
}
