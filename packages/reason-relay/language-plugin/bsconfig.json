{
  "name": "relay-compiler-language-reason",
  "version": "0.1.0",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": ["@gabnor/bs-flow-parser", "tablecloth-bucklescript"],
  "warnings": {
    "error": "+101"
  },
  "refmt": 3,
  "gentypeconfig": {
    "module": "commonjs",
    "language": "typescript",
    "shims": {
      "Js": "Js"
    },
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
